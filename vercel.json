{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(bundle.js|styles.css|assets/.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
